{
  "name": "ai-grading-assistant-v5",
  "version": "5.0.0",
  "description": "智学网AI智能阅卷助手 - Chrome扩展应用",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint ai-grading-extension/**/*.js",
    "lint:fix": "eslint ai-grading-extension/**/*.js --fix",
    "format": "prettier --write ai-grading-extension/**/*.js",
    "security-audit": "npm audit && node security-audit.js",
    "version:show": "node scripts/version-manager.js show",
    "version:sync": "node scripts/version-manager.js sync",
    "version:check": "node scripts/version-manager.js check",
    "version:bump:patch": "node scripts/version-manager.js bump patch",
    "version:bump:minor": "node scripts/version-manager.js bump minor",
    "version:bump:major": "node scripts/version-manager.js bump major",
    "release": "npm run version:check && npm run build && node scripts/version-manager.js bump patch",
    "release:minor": "npm run version:check && npm run build && node scripts/version-manager.js bump minor",
    "release:major": "npm run version:check && npm run build && node scripts/version-manager.js bump major"
  },
  "keywords": [
    "ai",
    "grading",
    "education",
    "chrome-extension",
    "zhixue"
  ],
  "author": "AI智能阅卷助手团队",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.22.0",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.5.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-extension-manifest-plugin": "^0.8.0"
  },
  "dependencies": {
    "dompurify": "^3.0.5",
    "jsencrypt": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/ai-grading-assistant-v5.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/ai-grading-assistant-v5/issues"
  },
  "homepage": "https://github.com/your-repo/ai-grading-assistant-v5#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
